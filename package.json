{
  "name": "react_base_project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "NODE_ENV=development && webpack -d && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline --hot",
    "build:prod": "NODE_ENV=production && webpack -p && cp src/index.html dist/index.html",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/enzyme.config.js",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "bower_components"
    ],
    "collectCoverage": true,
    "unmockedModulePathPatterns": [
      "react",
      "enzyme"
    ]
  },
  "dependencies": {
    "axios": "^0.17.1",
    "history": "^4.7.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "extract-text-webpack-plugin": "^3.0.0",
    "jest": "^22.1.4",
    "node-sass": "^4.9.0",
    "react-test-renderer": "^15.6.1",
    "react-tools": "^0.13.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.11.2"
  }
}
